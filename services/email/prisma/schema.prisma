
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Email {
  id String @id @default(cuid())
  sender String
  recipient String
  subject String
  body String
  source String
  sentAt DateTime @default(now())
}
